---
import portfolio from "../../assets/bestVideos.json";
import Video from "./Video.astro";
---

<ul class="video-list">
  {portfolio.map((video) => <Video id={video.id} title={video.title} />)}
</ul>

<style>
  .video-list {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    padding: 1.5em 0;
  }
</style>
